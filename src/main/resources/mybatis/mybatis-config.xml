<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC
"-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="useColumnLabel" value="true" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.team34.codehappy.member.Member" alias="Member"/>
		<typeAlias type="com.team34.codehappy.board.Board" alias="Board"/>
		<typeAlias type="com.team34.codehappy.board.Reply" alias="Reply"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/mappers/board-mapper.xml"/>
		<mapper resource="mybatis/mappers/member-mapper.xml"/>
		<mapper resource="mybatis/mappers/reply-mapper.xml"/>
	</mappers>
</configuration>